{
    "name": "wdio-boilerplate-cucumber",
    "version": "2.0.3",
    "description": "Everything you need to get started with selenium tests in node.js.",
    "repository": {
        "type": "git",
        "url": "https://github.com/dcypherthis/wdio-boilerplate-cucumber.git"
    },
    "main": "wdio.conf.js",
    "scripts": {
        "build": "babel emmapages/src --presets babel-preset-es2015 --out-dir dist",
        "cucumber": "node --harmony node_modules/.bin/cucumberjs test/features/ -r test/stepDefinitions",
        "legacy-cucumber": "node_modules/.bin/cucumberjs testLegacy/features/ -r testLegacy/support/stepDefinitions",
        "debug-local-dev": "DEBUG=true node  --harmony node_modules/.bin/wdio test/config/local/dev.conf.js",
        "lint": "node --harmony node_modules/.bin/eslint ./*",
        "legacy-lint": "node_modules/.bin/eslint testLegacy/support/*",
        "node-inspector": "node-inspector --debug-port 5859 --no-preload",
        "selenium": "node --harmony node_modules/.bin/selenium-standalone start",
        "selenium-install": "node --harmony node_modules/.bin/selenium-standalone install",
        "test": "node --harmony node_modules/.bin/wdio test/config/remote/prod.conf.js",
        "test-local": "node --harmony node_modules/.bin/wdio test/config/local/suite.conf.js",
        "test-local-dev": "node --harmony node_modules/.bin/wdio test/config/local/dev.conf.js",
        "test-remote": "node --harmony node_modules/.bin/wdio test/config/remote/suite.sauce.conf.js",
        "test-remote-dev": "node --harmony node_modules/.bin/wdio test/config/remote/dev.sauce.conf.js"
    },
    "keywords": [
        "test",
        "automation",
        "selenium",
        "webdriver",
        "webdriverIO"
    ],
    "homepage": "http://waffle.io/dcypherthis/wdio-boilerplate-cucumber",
    "bugs": {
        "url": "http://github.com/dcypherthis/wdio-boilerplate-cucumber/issues",
        "email": "jcypher@myemma.com"
    },
    "author": "Josh Cypher <jcypher@myemma.com> (http://github.com/dcypherthis/)",
    "contributors": [
        "Jackson Streeter <jstreeter@myemma.com> (http://github.com/streeter8/)"
    ],
    "license": "MIT",
    "dependencies": {
        "babel-preset-es2015": "6.9.0",
        "babel-preset-stage-0": "^6.3.13",
        "babel-register": "^6.11.5",
        "chai": "3.5.0",
        "chai-as-promised": "5.3.0",
        "cucumber": "^1.2.0",
        "deepmerge": "^0.2.10",
        "node-uuid": "^1.4.7",
        "q": "^1.4.1",
        "wdio-cucumber-framework": "0.2.4",
        "wdio-dot-reporter": "0.0.6",
        "wdio-json-reporter": "0.0.3",
        "wdio-sauce-service": "^0.2.4",
        "wdio-spec-reporter": "0.0.3",
        "webdriverio": "^4.2.3"
    },
    "devDependencies": {
        "eslint": "^3.0.1",
        "eslint-plugin-react": "^3.11.1",
        "selenium-standalone": "^5.5.0"
    }
}
